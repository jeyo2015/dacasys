@{
    ViewBag.Title = "Home Page";
    Layout = null;
}
<!DOCTYPE html>
<html lang="en" data-ng-app="WOdontoweb">
<head>
    <meta charset="utf-8">
    <meta http-equiv="cache-control" content="no-cache" />
    <title>.:: ODONTOWEB ::.</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <!-- CSS -->
    <link href="@Url.Content("~/Scripts/toastr.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Scripts/app/libs/Bootstrap/css/ripples.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Scripts/app/libs/Bootstrap/css/bootstrap.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Scripts/app/libs/Bootstrap/css/bootstrap-material-design.css")" rel="stylesheet" />
    <link href="@Url.Content("~")" id="basePath" rel="stylesheet" />
    <link href="@Url.Content("~/Content/fontAwesome/css/font-awesome.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/jquery.fileupload.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/jquery.fileupload-ui.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/jquery.fileupload-noscript.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/jquery.fileupload-ui-noscript.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/themes/base/jquery-ui.css")" rel="stylesheet" />
</head>
<body>
    <div id="divLoaderfull" class="loader" loader>
        <div>
            <span>
                <img src="@Url.Content("~/Content/img/ajax-loader.gif")" />Cargando.. </span>
        </div>
    </div>
    <div id="containerTotal">
        <div class="row" id="headerTotal">
            <div id="header-1">
                <!--Start Top Bar-->
                <div class="top-bar">
                    <div class="container">
                        <div class="row">
                            <div class="col-xs-3 col-md-3">
                                <img src="@Url.Content("~/Content/img/2.png")"  style="height: 37px; padding-top: 4px;"/>
                            </div>
                            <div class="col-xs-9 col-md-9">
                                <div class="get-touch">
                                    <ul>
                                        <li ng-hide="sessionDto.loginUsuario.length>0"><a ng-click=" showModal($event)"><i class="fa fa-sign-in"></i>Ingresar</a></li>
                                        <li class="dropdown" ng-show="sessionDto.loginUsuario.length>0 && sessionDto.IDConsultorio!=-1 && validarPermisoModulo('notificaciones')">
                                            <a class="dropdown-toggle" data-toggle="dropdown" ng-click="desabilitarNuevasNotificaciones($event)"><i class="fa fa-user-plus"></i><span ng-show="NotificacionesConsultorio.CantidadNuevasNotificaciones>0" class="badge badgePosition">{{NotificacionesConsultorio.CantidadNuevasNotificaciones}}</span></a>
                                            <ul id="menuPerfil" class="dropdown-menu pull-right" ng-show="validarPermisoFormulario('liNotificaciones')">
                                                <li class="withoutborderNS" ng-show="NotificacionesConsultorio.Notificaciones.length== 0"><a><span>No hay solicitudes pendientes</span> </a></li>
                                                <li ng-show="NotificacionesConsultorio.Notificaciones.length>0" ng-repeat=" noti in NotificacionesConsultorio.Notificaciones" class="withoutborderNS">
                                                    <a href="#">{{noti.NombreUsuario}}
                                                        <button class="btn btn-raised btn-primary buttonNotificacion" ng-click="confirmarSolicitud(noti, $event)" ng-disabled="!validarPermisoComponente('btnAceptarPaciente')">Confirmar</button>
                                                        <button class="btn btn-raised  btn-default buttonNotificacion" ng-click="cancelarSolicitud(noti, $event)" ng-disabled="!validarPermisoComponente('btnCancelarPaciente')">Eliminar solicitud</button>
                                                    </a>
                                                </li>
                                            </ul>
                                        </li>
                                        <li class="dropdown" ng-show="sessionDto.loginUsuario.length>0 && sessionDto.IDConsultorio!=-1 && validarPermisoModulo('configuracion')">
                                            <a class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-cog"></i><span class="caret"></span></a>
                                            <ul id="menuPerfil" class="dropdown-menu pull-right">
                                                <li class="withoutborderNS" ng-show="validarPermisoFormulario('liConfiguracionHorarios')"><a href="#horario"><span>Configurar horarios</span> </a></li>
                                                <li class="withoutborderNS" ng-show="validarPermisoFormulario('liConfigurarConsultorio')"><a href="#miConsultorioPerfil"><span>Configurar consultorio</span> </a></li>
                                            </ul>
                                        </li>
                                        <li class="dropdown" ng-show="sessionDto.loginUsuario.length>0 && sessionDto.IDConsultorio!=-1">
                                            <a class="dropdown-toggle" data-toggle="dropdown" href="#."><i class="fa fa-user"></i><span class="caret"></span></a>
                                            <ul id="menuPerfil" class="dropdown-menu pull-right">
                                                <li class="withoutborder"><a ng-click="openModalChangePass($event)" href="#">{{sessionDto.Nombre}}</a></li>
                                                <li class="withoutborder"><a ng-click="cerrarSesion($event)" href="#">Cerrar Sesion</a></li>
                                            </ul>
                                        </li>
                                        <li class="dropdown" ng-show="sessionDto.loginUsuario.length>0 && sessionDto.IDConsultorio==-1">
                                            <a class="dropdown-toggle" data-toggle="dropdown" href="#."><i class="fa fa-user"></i><span class="caret"></span></a>
                                            <ul id="menuPerfil" class="dropdown-menu pull-right">
                                                <li class="withoutborder"><a ng-click="openModalEditarPaciente($event)" href="#">{{sessionDto.Nombre}}</a></li>
                                                <li class="withoutborder"><a ng-click="cerrarSesion($event)" href="#">Cerrar Sesion</a></li>
                                            </ul>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!--Top Bar End-->
                <!--Start Header-->
                <header class="header">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-12 col-xs-12">
                                <div class="navbar navbar-default ">
                                    <div class="container-fluid">
                                        <div class="navbar-header">
                                            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-responsive-collapse">
                                                <span class="icon-bar"></span>
                                                <span class="icon-bar"></span>
                                                <span class="icon-bar"></span>
                                            </button>
                                        </div>
                                        <div class="navbar-collapse collapse navbar-responsive-collapse">
                                            <ul class="nav navbar-nav">
                                                <li ng-class="getClass('/inicioCliente')" ng-hide="!sessionDto || sessionDto.IDRol != -1"><a href="#inicioCliente"><i class="fa fa-home iconMenu "></i>Inicio</a></li>
                                                <li ng-class="getClass('/consultas')" ng-hide="sessionDto.IDConsultorio==-1 || !validarPermisoModulo('consultas')"><a href="#consultas"><i class="fa fa-calendar iconMenu "></i>Consultas</a></li>
                                                <li ng-class="getClass('/cuentas')" ng-hide="sessionDto.IDConsultorio==-1 || !validarPermisoModulo('cuentas')"><a href="#cuentas"><i class="fa fa-usd iconMenu "></i>Cuentas por Cobrar</a></li>
                                                <li ng-class="getClass('/usuarios')" ng-hide="sessionDto.IDConsultorio==-1 || !validarPermisoModulo('usuarios')"><a href="#usuarios"><i class="fa fa-users iconMenu "></i>Usuarios</a></li>
                                                <li ng-class="getClass('/roles')" ng-hide="sessionDto.IDConsultorio==-1 || !validarPermisoModulo('roles')"><a href="#roles"><i class="fa fa-user-md iconMenu "></i>Perfiles</a></li>
                                                <li ng-class="getClass('/consultorios')" ng-show="sessionDto.IsDacasys && validarPermisoModulo('consultorios') && sessionDto.IDConsultorio!=-1 "><a href="#consultorios"><i class="fa fa-hospital-o iconMenu "></i>Clinicas</a></li>
                                                <li ng-class="getClass('/paciente')" ng-hide="sessionDto.IDConsultorio==-1 || !validarPermisoModulo('paciente')"><a href="#paciente"><i class="fa fa-user iconMenu "></i>Pacientes</a></li>
                                                <li ng-class="getClass('/comentario')" ng-hide="sessionDto.IDConsultorio==-1 || !validarPermisoModulo('comentarios')"><a href="#comentario"><i class="fa fa-align-justify iconMenu "></i>Comentarios</a></li>

                                                <li ng-class="getClass('/misCitas')" ng-show="sessionDto.IDConsultorio==-1 && sessionDto.loginUsuario.length>0"><a href="#misCitas"><i class="fa fa-calendar-plus-o iconMenu "></i>Mis citas</a></li>
                                                <li ng-class="getClass('/cuentasPorPagar')" ng-show="sessionDto.IDConsultorio==-1 && sessionDto.loginUsuario.length>0"><a href="#cuentasPorPagar"><i class="fa fa-usd iconMenu "></i>Mis cuentas por pagar</a></li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </header>
            </div>
        </div>
        <div ng-view class=" row">
        </div>
        <div class=" row" style="text-align: center;">
            <span class="text-center" style="color: #3B3B3C; font-family: Tahoma; font-size: 11px; font-weight: bold; text-align: center;">Desarrollado por DACASYS</span>
        </div>
        <div class=" row" style="text-align: center;">
            <span style="color: #3B3B3C; font-family: Tahoma; font-size: 11px; font-weight: bold; text-align: center;"><a href="http://www.dacasys.com">www.dacasys.com</a>  - 77390401 - 65051550</span>
        </div>
    </div>
    <div ng-controller="loginController">
        <div class="modal fade" id="modal-login" data-backdrop="static" role="dialog">
            <div class="modal-dialog modal-sm">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
                        <h4 class="modal-title">Ingresar a Odontoweb</h4>
                    </div>
                    <div class="modal-body">
                        <form class="form-horizontal" autocomplete="off">
                            <input style="display: none">
                            <input type="password" autocomplete="off" style="display: none" class="form-control" value="" id="xxx">
                            <input type="text" autocomplete="off" style="display: none" class="form-control" value="" id="xxx">

                            <div class="row" id="formLogin">
                                <div class="col-md-12">
                                    <div class="form-group label-floating" ng-show="isAdmin" id="formGroupConsultorio">
                                        <label for="consultorioID" class="col-md-2 control-label">Consultorio</label>

                                        <div class="col-md-12">
                                            <input type="text" ng-model="loginEmpresa" class="form-control" id="consultorioID" autocomplete="off">
                                        </div>
                                        <div class="col-md-10">
                                            <span style="color: #D82756; font-size: 11px" ng-show="sessionDto.Verificar==0 || sessionDto.Verificar==1">{{message}} </span>
                                        </div>
                                    </div>
                                    <div class="form-group label-floating" id="formGroupUsuario">
                                        <label for="consultorioID" class="col-md-2 control-label">Usuario</label>

                                        <div class="col-md-12">
                                            <input type="text" ng-model="usuario" class="form-control" id="usuarioID" autocomplete="false">
                                        </div>
                                        <div class="col-md-10">
                                            <span style="color: #D82756; font-size: 11px" ng-show="sessionDto.Verificar==2 ">{{message}} </span>
                                        </div>
                                    </div>
                                    <div class="form-group label-floating" id="formGroupPass">
                                        <label for="consultorioID" class="col-md-2 control-label">Contrasena</label>
                                        <div class="col-md-12">
                                            <input type="password" ng-model="pass" autocomplete="off" class="form-control" value="" id="passwordIDLog" ng-keypress="enterLogIn($event)">
                                        </div>
                                        <div class="col-md-10">
                                            <span style="color: #D82756; font-size: 11px" ng-show="sessionDto.Verificar==4">{{message}} </span>
                                        </div>
                                    </div>
                                    <div class="col-md-12 linkForgotPass">
                                        <a ng-click=" renovarContrasena()" style="color: #D82756; font-size: 11px">Olvidaste tu constraseña?</a>
                                    </div>
                                    <div class="checkbox" style="margin-top: 20px">
                                        <label>
                                            <input ng-model="isAdmin" ng-click="cleanInputs()" type="checkbox"><span style="margin-right: 4px" class="check"></span> Soy Clinica
                                        </label>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="modal-footer">
                         <button class="btn btn-primary" ng-click="openModalregistrarCliente()">Registrarse</button>
                        <button class="btn btn-primary" ng-click="ingresar()" ng-disabled="validarCampos()">Ingresar</button>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal fade" id="modal-registrarse" role="dialog">
            <div class="modal-dialog ">
                <div class="modal-content">
                    <div class="modal-header">
                        <h4 class="modal-title">Registrarse a Odontoweb</h4>
                    </div>
                    <div class="modal-body">
                        <form class="form-horizontal">
                            <div class="form-group">
                                <label for="login" class="col-md-3 control-label">Login:</label>
                                <div class="col-md-9">
                                    <input type="text" maxlength="20" ng-model="pacienteParaGuardar.LoginCliente" class="form-control" id="login" ng-disabled="validarCliente()">
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="ci" class="col-md-3 control-label">CI:</label>
                                <div class="col-md-9">
                                    <input type="text" maxlength="10" ng-model="pacienteParaGuardar.Ci" class="form-control" id="ci">
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="nombre" class="col-md-3 control-label">Nombre:</label>
                                <div class="col-md-9">
                                    <input type="text" maxlength="30" ng-model="pacienteParaGuardar.Nombre" class="form-control" id="nombre">
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="apellido" class="col-md-3 control-label">Apellido:</label>
                                <div class="col-md-9">
                                    <input type="text" maxlength="30" ng-model="pacienteParaGuardar.Apellido" class="form-control" id="apellido">
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="direccion" class="col-md-3 control-label">Direccion:</label>
                                <div class="col-md-9">
                                    <input type="text" maxlength="30" ng-model="pacienteParaGuardar.Direccion" class="form-control" id="direccion">
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="email" class="col-md-3 control-label">Email:</label>
                                <div class="col-md-9">
                                    <input type="text" maxlength="30" ng-model="pacienteParaGuardar.Email" class="form-control" id="email">
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="antecedentes" class="col-md-3 control-label">Antecedentes:</label>
                                <div class="col-md-9">
                                    <input type="text" maxlength="50" ng-model="pacienteParaGuardar.Antecedentes" class="form-control" id="antecedentes">
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="telefono" class="col-md-3 control-label">Telefono:</label>
                                <div class="col-md-9">
                                    <input type="text" maxlength="10" ng-model="pacienteParaGuardar.Telefono" class="form-control" id="telefono">
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="selectSexo" class="col-md-3 control-label">Genero:</label>
                                <div class="col-md-9">
                                    <select class="form-control" id="selectSexo" ng-model="selectSexo">
                                        <option value="F" selected="selected">Femenino</option>
                                        <option value="M">Masculino</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="selectTipoSangre" class="col-md-3 control-label">Tipo Sangre:</label>
                                <div class="col-md-9">
                                    <select class="form-control" id="selectTipoSangre" ng-model="selectTipoSangre">
                                        <option value="O+" selected="selected">O+</option>
                                        <option value="A+">A+</option>
                                        <option value="B+">B+</option>
                                        <option value="O-">O-</option>
                                    </select>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="modal-footer">
                        <button class="btn btn-primary" ng-click="registrarCliente()" ng-disabled="validarCamposPaciente()">Aceptar</button>

                        <button class="btn btn-primary" ng-click="cerrarModalRegistrar()">Cancelar</button>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal fade" id="modal-login-cliente" role="dialog">
            <div class="modal-dialog modal-sm">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
                        <h4 class="modal-title">Inicie sesion para continuar</h4>
                    </div>
                    <div class="modal-body">
                        <form class="form-horizontal">
                            <input style="display: none">
                            <input type="password" autocomplete="off" style="display: none" class="form-control" value="" id="xxx5">
                            <input type="text" autocomplete="off" style="display: none" class="form-control" value="" id="xxx3">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group label-floating">
                                        <label for="consultorioID" class="col-md-2 control-label">Usuario</label>

                                        <div class="col-md-12">
                                            <input type="text" ng-model="usuario" class="form-control" id="usuarioID" autocomplete="off">
                                        </div>
                                        <div class="col-md-10">
                                            <span style="color: #D82756; font-size: 11px" ng-show="sessionDto.Verificar==2 ">{{message}} </span>
                                        </div>
                                    </div>
                                    <div class="form-group label-floating">
                                        <label for="consultorioID" class="col-md-2 control-label">Contrasena</label>
                                        <div class="col-md-12">
                                            <input type="password" ng-model="pass" class="form-control" id="passwordID" ng-keypress="enterLogIn($event)">
                                        </div>
                                        <div class="col-md-10">
                                            <span style="color: #D82756; font-size: 11px" ng-show="sessionDto.Verificar==4">{{message}} </span>
                                        </div>
                                    </div>
                                    <div class="col-md-12 linkForgotPass">
                                        <a ng-click=" renovarContrasena()" style="color: #D82756; font-size: 11px">Olvidaste tu constraseña?</a>
                                    </div>

                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="modal-footer">
                        <button class="btn btn-primary" ng-click="openModalregistrarCliente()">Registrarse</button>

                        <button class="btn btn-primary" ng-click="ingresar()" ng-disabled="validarCamposInicioCliente()">Ingresar</button>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal fade" id="modal-renovar" role="dialog">
            <div class="modal-dialog modal-sm">
                <div class="modal-content">
                    <div class="modal-header">
                        <h4 class="modal-title">Renovar contrasena</h4>
                    </div>
                    <div class="modal-body">
                        <form class="form-horizontal">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group ">
                                        <label for="consultorioID" class="col-md-2 control-label">Usuario</label>
                                        <div class="col-md-12">
                                            <input type="text" ng-disabled="true" ng-model="usuario" class="form-control" id="loginID">
                                        </div>
                                    </div>
                                    <div class="form-group label-floating">
                                        <label for="newPasswordID" class="col-md-2 control-label">Contrasena</label>
                                        <div class="col-md-12">
                                            <input type="password" ng-model="newPass" class="form-control" id="newPasswordID">
                                        </div>
                                        <div class="col-md-10">
                                            <span style="color: #D82756; font-size: 11px" ng-show="showMessage">{{message}} </span>
                                        </div>
                                    </div>
                                    <div class="form-group label-floating">
                                        <label for="confirmPasswordID" style="text-align: left;" class="col-md-8 control-label">Repita Contrasena</label>
                                        <div class="col-md-12">
                                            <input type="password" ng-model="ConfirmPass" class="form-control" id="confirmPasswordID">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="modal-footer">
                        <button class="btn btn-primary" ng-click="changePassUser()" ng-disabled="validarCampos()">Aceptar</button>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal fade" id="modal-mi-perfil" role="dialog">
            <div class="modal-dialog ">
                <div class="modal-content">
                    <div class="modal-header">
                        <h4 class="modal-title">Mi perfil</h4>
                    </div>
                    <div class="modal-body">
                        <form class="form-horizontal">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group ">
                                        <label for="loginuserID" class="col-md-3 control-label">Login</label>
                                        <div class="col-md-9">
                                            <input type="text" ng-disabled="true" ng-model="sessionDto.loginUsuario" class="form-control col-md-11" id="loginuserID">
                                        </div>
                                    </div>
                                    <div class="form-group ">
                                        <label for="nombreUserID" class="col-md-3 control-label">Nombre</label>

                                        <div class="col-md-9">
                                            <input type="text" ng-model="userToSave.Nombre" class="form-control" id="nombreUserID">
                                        </div>

                                    </div>
                                    <div class="form-group ">
                                        <label for="newPasswordPerfilID" class="col-md-3 control-label">Contrasena</label>

                                        <div class="col-md-9">
                                            <input type="password" ng-model="userToSave.Password" class="form-control" id="newPasswordPerfilID">
                                        </div>
                                        <div class="col-md-9">
                                            <span style="color: #D82756; font-size: 11px" ng-show="showMessage">{{message}} </span>
                                        </div>
                                    </div>
                                    <div class="form-group ">
                                        <label for="confirmPasswordPerfilID" class="col-md-3 control-label">Repita Contrasena</label>

                                        <div class="col-md-9">
                                            <input type="password" ng-model="userToSave.ConfirmPass" class="form-control col-md-12" id="confirmPasswordPerfilID">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="modal-footer">
                        <button class="btn btn-primary" ng-click="updateUser()" ng-disabled="validarCamposPerfil()">Aceptar</button>
                        <button class="btn btn-primary" ng-click="closeModal('#modal-mi-perfil')">Cancelar</button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div ng-controller="editarPacienteController">
        <div class="modal fade" id="modal-editar-paciente" data-backdrop="static" role="dialog">
            <div class="modal-dialog ">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
                        <h4 class="modal-title">Datos paciente</h4>
                    </div>
                    <div class="modal-body">
                        <form class="form-horizontal">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label for="loginEditarPaciente" class="col-md-3 control-label">Login:</label>
                                        <div class="col-md-9">
                                            <input type="text" maxlength="20" ng-model="editarPacienteParaGuardar.LoginCliente" class="form-control" id="loginEditarPaciente" ng-disabled="true">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="ciEditarPaciente" class="col-md-3 control-label">CI:</label>
                                        <div class="col-md-9">
                                            <input type="text" maxlength="10" ng-model="editarPacienteParaGuardar.Ci" class="form-control" id="ciEditarPaciente">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="nombreEditarPaciente" class="col-md-3 control-label">Nombre:</label>
                                        <div class="col-md-9">
                                            <input type="text" maxlength="30" ng-model="editarPacienteParaGuardar.Nombre" class="form-control" id="nombreEditarPaciente">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="apellidoEditarPaciente" class="col-md-3 control-label">Apellido:</label>
                                        <div class="col-md-9">
                                            <input type="text" maxlength="30" ng-model="editarPacienteParaGuardar.Apellido" class="form-control" id="apellidoEditarPaciente">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="direccionEditarPaciente" class="col-md-3 control-label">Direccion:</label>
                                        <div class="col-md-9">
                                            <input type="text" maxlength="30" ng-model="editarPacienteParaGuardar.Direccion" class="form-control" id="direccionEditarPaciente">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="emailEditarPaciente" class="col-md-3 control-label">Email:</label>
                                        <div class="col-md-9">
                                            <input type="text" maxlength="30" ng-model="editarPacienteParaGuardar.Email" class="form-control" id="emailEditarPaciente">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="telefonoEditarPaciente" class="col-md-3 control-label">Telefono:</label>
                                        <div class="col-md-9">
                                            <input type="text" maxlength="10" ng-model="editarPacienteParaGuardar.Telefono" class="form-control" id="telefonoEditarPaciente">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="selectSexoEditarPaciente" class="col-md-3 control-label">Genero:</label>
                                        <div class="col-md-9">
                                            <select class="form-control" id="selectSexoEditarPaciente" ng-model="selectSexoEditarPaciente">
                                                <option value="F" selected="selected">Femenino</option>
                                                <option value="M">Masculino</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="selectTipoSangreEditarPaciente" class="col-md-3 control-label">Tipo Sangre:</label>
                                        <div class="col-md-9">
                                            <select class="form-control" id="selectTipoSangreEditarPaciente" ng-model="selectTipoSangreEditarPaciente">
                                                <option value="O+" selected="selected">O+</option>
                                                <option value="A+">A+</option>
                                                <option value="B+">B+</option>
                                                <option value="O-">O-</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="passwordEditarPaciente" class="col-md-3 control-label">Cambiar Contrasena:</label>
                                        <div class="col-md-9">
                                            <input type="password" maxlength="10" ng-model="selectPassword" class="form-control" id="passwordEditarPaciente">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="modal-footer">
                        <button class="btn btn-primary" ng-click="modificarEditarPaciente()" ng-disabled="validarCamposEditarPaciente()">Aceptar</button>
                        <button class="btn btn-primary" ng-click="closeModal('#modal-editar-paciente')">Cancelar</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
